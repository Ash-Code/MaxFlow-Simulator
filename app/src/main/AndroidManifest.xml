<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.maxflow" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/flowicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Input"
            android:label="@string/title_activity_input"
            android:parentActivityName=".MyActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.administrator.maxflow.MyActivity" />
        </activity>
        <activity
            android:name=".Add"
            android:label="@string/title_activity_add"
            android:parentActivityName=".Input" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.administrator.maxflow.Input" />
        </activity>
    </application>

</manifest>
